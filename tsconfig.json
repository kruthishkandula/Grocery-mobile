{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@atom": [
        "./app/components/atom/index"
      ],
      "@atom/*": [
        "./app/components/atom/*"
      ],
      "@molecule": [
        "./app/components/molecule/index"
      ],
      "@molecule/*": [
        "./app/components/molecule/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ]
}
